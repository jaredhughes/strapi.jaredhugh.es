{
  "connection": "default",
  "collectionName": "locations",
  "info": {
    "name": "location",
    "description": "Public location data"
  },
  "options": {
    "increments": true,
    "timestamps": ["created_at", "updated_at"],
    "comment": ""
  },
  "attributes": {
    "city": {
      "required": true,
      "type": "string"
    },
    "country": {
      "required": true,
      "type": "string"
    },
    "latitude": {
      "required": true,
      "type": "string"
    },
    "longtitude": {
      "required": true,
      "type": "string"
    }
  }
}
